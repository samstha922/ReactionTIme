ethods:{\n        startTimer(){\n            this.timer= setInterval(()=>{\n                this.reactionTime += 10\n            },10)\n        },\n        stopTimer(){\n            clearInterval(this.timer)\n            // console.log('reaction time: '+this.reactionTime)\n            this.$emit('endTime', this.reactionTime)\n        }\n\n    }\n    // updated(){\n    //     console.log('updated') \n    // },\n    // unmounted(){\n    //     console.log('unmounted')\n    // }\n}\n</script>\n\n<style scoped>\n#square{\n    position: absolute;\n    top: 50%;\n    left: 50%;\n    margin-top: -100px;\n    margin-left: -100px;\n    width: 200px;\n    height: 200px;\n    background: yellow; \n    color: black;\n}\n#square p{\n    margin: 0;\n    position: absolute;               /* 2 */\n    top: 50%; \n    left: 20%;\n}\n</style>"]}]}